<%- include ("partials/header")%>
<a style="color: white; font-size: 20px;" href="/">&larr; Back</a>
<div style="padding-top: 0; padding-bottom: 0px;" class="dash_head">
  <p class="dash_p">Login</p>
</div>
<div class="login-layout">
  <%- include ("partials/messages")%>
  <div class="form">
    <form action="/users/login" method="POST" class="form form-login" autocomplete="off">
      <div class="form-entries">
        <input
          type="email"
          name="email"
          id="email"
          placeholder="Enter Your Mail ID"
          
        />
      </div>
      <div class="form-entries">
        <input
          type="password"
          name="password"
          id="password"
          placeholder="Enter Your Passsword"
        />
      </div>
      <div style="margin-bottom: 20px;" class="form-entries">
        <button  type="submit" class="sub_btn alter">Submit</button>
      </div>
    </form>
  </div>
  <div class="alter">
    New Here? <a href="/users/register/normal-user">Register</a>
  </div>
</div>

<%- include ("partials/footer")%>  
